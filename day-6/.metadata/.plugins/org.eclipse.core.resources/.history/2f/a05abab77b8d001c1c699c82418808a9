#include <stdio.h>
#include <stdlib.h>

int stringLenght(char str[]);

int main(void) {
	char str[] = "!!!Hello World!!!";
	printf("%d", stringLenght(str));

	return 0;

}



int stringLenght(char str[])
{
	int lenght=1;
	int i = 0;
	while(str[i]!= '\0')
	{
		lenght++;
		i++;
	}
	return 0;
}
